<h1>{{title}}</h1>
<div >
	{{#if productos}}
        <table>
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Imagen</th>
            </tr>
		    {{#each productos}}
			    <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td>
                        <img src={{this.thumbnail}} style="width: 50px;"></img>
                    </td>
                </tr>
		    {{/each}}
        </table>
	{{else}}
		<span>No hay productos</span>
	{{/if}}

</div>
    <a href="/" >Agregar Producto</a>